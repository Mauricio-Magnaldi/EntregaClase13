<form action="/api/users/signup" method="POST"> 
    Nombre: <input type="text" name="first_name" id="first_name">
    Apellido: <input type="text" name="last_name" id="last_name">
    Email: <input type="email" name="email" id="email">
    Edad: <input type="number" name="age" id="age">
    Password: <input type="password" name="password" id="password">
    CartId: <input type="number" name="cart_id" id="cart_id">
    Rol: <input type="text" name="role" id="role">
    <input type="submit" value="Registro">
</form>